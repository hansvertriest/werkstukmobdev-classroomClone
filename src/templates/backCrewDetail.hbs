<div class="o-app-container">
	{{#with data }}
	<h1>{{crewCode}}</h1>
	{{/with}}
	<p>{{status}}</p>
	{{#with data }}
	<ul class="m-member-list">
	{{#members}}
	<li class="m-member-list__member">
		<img class="a-avatar__pic" src="../assets/images/avatar/astro1_128.png"></img>
		<p class="a-member__screenName">{{screenName}}</p>
		<button id="a-delete-{{userId}}">X</button>
		<button id="a-addTagger-{{userId}}">set tagger</button>
	</li>
	{{/members}}
	</ul>
	{{/with}}
	<button class="a-backend__button" id="{{addUserBtnId}}">Add user</button>
	<button class="a-backend__button" id="{{forceStartButtonId}}">{{startStopInnerText}}</button>
	<button class="a-backend__button" id="{{simulateGameId}}">Simulate game</button>
	<button class="a-map-btn" id="{{mapBtnId}}">Go to map</button>
</div>